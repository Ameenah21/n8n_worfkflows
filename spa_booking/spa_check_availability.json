    {
      "parameters": {
        "jsCode": "const events = $input.all();\nconst therapistName = $('Loop Over Items').item.json['therapist_name'];\nconst calendarId = $('Loop Over Items').item.json['GCal ID'];\nconst isAvailable = $('Get availability in a calendar').item.json['available'];\n\n// If no events returned, barber is available\n// const isAvailable = events.length === 0;\nif (isAvailable === 'True');\n  return [{\n    json: {\n      therapist_name: therapistName,\n      calendar_id: calendarId,\n      available: isAvailable,\n      conflicting_events: events.length,\n      checked_date: $('Parse Input').first().json.date,\n      checked_time: $('Parse Input').first().json.time\n    }\n}];"
      },
      "type": "n8n-nodes-base.code",
