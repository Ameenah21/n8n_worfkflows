      ],
      "id": "c9c441c9-d08e-431e-b4e4-8b6fbd4f3a97",
      "name": "Determine Availability"
    },
    {
      "parameters": {
        "jsCode": "const allResults = $('Loop Over Items').all();\n\nconst availabletherapists = allResults.filter(r => r.json.available);\nconst busytherapists = allResults.filter(r => !r.json.available);\n\nreturn [{\n  json: {\n    requested_date: $('Parse Input').first().json.date,\n    requested_time: $('Parse Input').first().json.time,\n    requested_service: $('Parse Input').first().json.service,\n    therapist_preference: $('Parse Input').first().json.therapist_preference,\n    available_therapists: availabletherapists.map(b => ({\n      name: b.json.therapist_name,\n      calendar_id: b.json.calendar_id\n    })),\n    busy_therapists: busytherapists.map(b => ({\n      name: b.json.therapist_name,\n      calendar_id: b.json.calendar_id\n    })),\n    any_available: availabletherapists.length > 0\n  }\n}];"
      },
