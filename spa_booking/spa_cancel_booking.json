    {
      "parameters": {
        "jsCode": "const booking = $input.first().json;\nconst validationData = $('When Executed by Another Workflow').first().json;\n\nif (!booking || !booking.booking_id) {\n  return [{\n    json: {\n      error: true,\n      message: \"Booking not found\",\n      code: \"BOOKING_NOT_FOUND\",\n      booking_id: validationData.booking_id,\n      books: booking\n    }\n  }];\n}\n\nif (booking.status === 'cancelled') {\n  return [{\n    json: {\n      error: true,\n      message: \"Booking is already cancelled\",\n      code: \"ALREADY_CANCELLED\",\n      booking_id: booking.booking_id\n    }\n  }];\n}\n\nreturn [{\n  json: {\n    error: false,\n    booking: booking\n  }\n}];"
      },
