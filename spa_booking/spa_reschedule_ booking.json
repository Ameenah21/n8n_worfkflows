    {
      "parameters": {
        "jsCode": "// Get all bookings and selected index from input\nconst bookings = $input.all() || [];\nconst selectedIndex = parseInt($('When Executed by Another Workflow').first().json.selected_index, 10) - 1; // 1-based index\nif (!bookings.length) {\n  return [{ json: { error: \"No bookings found\" } }];\n}\n\nif (selectedIndex < 0 || selectedIndex >= bookings.length) {\n  return [{ json: { error: \"Invalid booking index\" } }];\n}\n\nconst selected = bookings[selectedIndex];\n\n//Format booking info for rescheduling\nreturn [{\n  json: {\n    gcal_uid: selected.json.gcal_uid,\n    calendar_event_id: selected.json.calendar_event_id,\n    service: selected.json.service,\n    therapist_name: selected.json.therapist_name,\n    shop_name: selected.json.shop_name,\n    customer_email: selected.json.email,\n    customer_name: selected.json.customer_name,\n    old_date: selected.json.date,\n    old_time: selected.json.time,\n    new_date:$('When Executed by Another Workflow').first().json.new_date,\n    new_time: $('When Executed by Another Workflow').first().json.new_time,\n    status: \"ready_for_reschedule\",\n    selectedIndex\n  }\n}];\n"
      },
