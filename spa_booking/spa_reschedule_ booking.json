    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const input = item.json;\n\n  const requiredFields = [\n    \"customer_phone\",\n    \"new_date\",\n    \"new_time\",\n    \"selected_index\"\n  ];\n\n  // Find missing or null fields\n  const missing = requiredFields.filter(\n    f => input[f] === null || input[f] === undefined || input[f] === \"\"\n  );\n\n  if (missing.length > 0) {\n    return {\n      json: {\n        status: \"error\",\n        message: `Missing or null values for: ${missing.join(\", \")}`\n      }\n    };\n  }\n\n  // Return parsed and validated data\n  return {\n    json: {\n      status: \"ok\",\n      customer_phone: input.customer_phone,\n      service: input.service,\n      service_duration: input.service_duration,\n      barber_name: input.barber_name,\n      date: input.new_date,\n      time: input.new_time,\n      selected_index: input.selected_index\n    }\n  };\n});\n"
      },
